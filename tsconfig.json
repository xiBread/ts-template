{
	"compilerOptions": {
		"noEmit": true,
		"outDir": "dist",
		"strict": true,
		"skipLibCheck": true,
		"target": "ES2022",
		"module": "ESNext",
		"moduleDetection": "force",
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"resolveJsonModule": true
	},
	"include": [
		"**/*.ts"
	]
}
